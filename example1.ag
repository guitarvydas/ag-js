<html> <head>
    <title>Simple Example Concurrent File Loader</title>
  </head>

  <body>
    <h1></h1>
      <div>
        <ag-input type="file" id="fileToLoad1" onchange="fileToLoad1.react({pin:'changed',data:null})"></ag-input>
	<ag-p id="text1"></ag-p>
	<schematic id="top">
	  <part ref="text" />
	  <part ref="fileToLoad1" />
	  <wire>
	    <sender ref="fileToLoad1" pin="filedescriptor" />
	    <receiver ref="text1" pin="setText" />
	  </wire>
	</schematic>
      </div>
  </body>
</html>
